{
	"name": "app",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"start:test": "npm run dev -- --mode test",
		"start:pro": "npm run dev -- --mode production",
		"build": "vue-tsc --noEmit && vite build",
		"build:dev": "npm run build -- --mode development",
		"build:test": "npm run build -- --mode test",
		"build:pro": "npm run build -- --mode production",
		"preview": "vite preview",
		"lint": "eslint --ext .js,.vue,.ts src --fix",
		"tsc": "tsc -t es5 ./src/tsLearnCase/index.ts && node ./src/tsLearnCase/index.js",
		"prepare": "husky install",
		"commit": "git-cz"
	},
	"dependencies": {
		"amfe-flexible": "^2.2.1",
		"axios": "^0.27.2",
		"mockjs": "^1.1.0",
		"pinia": "^2.0.22",
		"qs": "^6.11.0",
		"vant": "^3.6.2",
		"vconsole": "^3.14.6",
		"vue": "^3.2.40",
		"vue-router": "^4.1.5"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.1.0",
		"@types/node": "^18.7.15",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"@vitejs/plugin-vue": "^3.0.3",
		"autoprefixer": "^10.4.8",
		"commitizen": "^4.2.5",
		"commitlint": "^17.1.2",
		"commitlint-config-cz": "^0.13.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.4.0",
		"husky": "^8.0.1",
		"less": "^4.1.3",
		"lint-staged": "^13.0.3",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^2.7.1",
		"rollup-plugin-visualizer": "^5.8.1",
		"typescript": "^4.6.4",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-vue-components": "^0.22.4",
		"vite": "^3.0.7",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-mock": "^2.9.6",
		"vite-plugin-vconsole": "^1.2.2",
		"vue-tsc": "^0.39.5"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	}
}
